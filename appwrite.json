{
  "projectId": "stack-compare",
  "projectName": "StackCompare",
  "functions": [
    {
      "functionId": "github-fetcher",
      "name": "GitHub Repository Fetcher",
      "runtime": "node-18.0",
      "execute": ["any"],
      "timeout": 300,
      "enabled": true,
      "logging": true,
      "entrypoint": "index.js",
      "path": "appwrite/functions/github-fetcher"
    }
  ],
  "databases": [
    {
      "databaseId": "stack-compare-db",
      "name": "StackCompare Database",
      "collections": [
        {
          "collectionId": "repositories",
          "name": "Repositories",
          "permissions": ["read(\"any\")", "create(\"any\")"],
          "attributes": [
            {
              "key": "repo_full_name",
              "type": "string",
              "required": true,
              "size": 255
            },
            {
              "key": "name",
              "type": "string",
              "required": true,
              "size": 255
            },
            {
              "key": "language",
              "type": "string",
              "size": 50
            }
          ]
        }
      ]
    }
  ]
}
